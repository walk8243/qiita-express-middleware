{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Qiita Express Middleware",
      "program": "${workspaceFolder}/node_modules/express-middleware-server/dist/server.js",
      "autoAttachChildProcesses": true,
      "env": {
        "CLUSTER": "2",
        "PORT": "3000",
        "ROUTER": "./dist/router.js"
      },
      "preLaunchTask": "Middleware Debug Build",
      "console": "internalConsole"
    }
  ]
}
